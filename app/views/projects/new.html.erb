<div class="text-center">
	<h1>Create a new project</h1>

	<%= form_for @project do |f| %>
		<form>
			<div>
				<%= f.label :name %>
				<%= f.text_field :name %>
			</div>
			<div>
				<%= f.label :description %>
				<%= f.text_area :description %>
			</div>

			<h2>Rewards</h2>
				<%= f.fields_for :rewards do |rewards_form| %>

				<% end %>

				
			<%= f.submit %>
		</form>
	<% end %>
</div>